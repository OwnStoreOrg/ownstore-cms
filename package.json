    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-collapsible": "^2.8.4",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.1.1",
    "ua-parser-js": "^0.7.28",
    "use-ssr": "^1.0.24"
  },
  "devDependencies": {
    "@heroicons/react": "^1.0.4",
